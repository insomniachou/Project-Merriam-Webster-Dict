{
  "manifest_version": 2,
  "name": "Merriam-Webster Dictionary Definition",
  "description": "This extension shows definition of selected word from Merriam-Webster Web API.",
  "version": "1.1",
  "background": {
    "scripts": ["jquery-3.3.1.min.js", "background.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["content_scripts.css"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": {
    "128": "128.png"
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ]
}
